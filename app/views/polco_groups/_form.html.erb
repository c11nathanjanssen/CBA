<%= simple_form_for(@polco_group) do |f| %>
  <%= f.error_messages %>
  <%= f.input :name %>
  <%= f.input :type,  :collection => [:custom, :state, :district] %>
  <%= f.input :description, :as => :text %>
  <%= f.input :owner, :input_html => { :value => current_user.id } %>
  <%= f.button :submit %>
<% end %>
